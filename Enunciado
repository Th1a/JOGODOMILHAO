package com.mycompany.jogodomilhao;

/**
 *
 * @author Swat
 */
public class Enunciado {
    
    protected String enunciado;
    
    public Enunciado(String enunciado){
        this.enunciado = enunciado;
    }
    public String getEnuciado() {
        return enunciado;
    }
    public void setEnuciado(String enunciado){
        this.enunciado = enunciado;
    }
}
